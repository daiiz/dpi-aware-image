{
  "name": "dpi-aware-image",
  "version": "2.0.5",
  "description": "DPI aware image CustomElement",
  "main": "lib/index.js",
  "scripts": {
    "demo": "node -r esm demo/server.mjs",
    "watch": "run-p watch:*",
    "watch:demo": "watchify demo/src/index.js -v -t babelify -o demo/dist/demo.js",
    "build:babel": "./node_modules/.bin/babel src/ --out-dir lib/",
    "build:browserify": "browserify src/index.js -v -t babelify -o lib/index.js",
    "postinstall": "npm run build:babel"
  },
  "author": "Daiki Iizuka",
  "license": "MIT",
  "repository": "git@github.com:daiiz/pHYs-png.git",
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "png-dpi-reader-writer": "0.0.4"
  },
  "devDependencies": {
    "esm": "^3.0.84",
    "polka": "^0.5.1",
    "serve-static": "^1.13.2",
    "npm-run-all": "^4.1.5",
    "watchify": "^3.11.0"
  }
}
